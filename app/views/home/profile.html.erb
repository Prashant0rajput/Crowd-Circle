<main>
<div class = "container">
	<div class="jumbo ">
    <%= image_tag @user.coverImage ,:class => "z-depth-2"%>
  </div>
	<div class="container icons ">
		<div class="big-icon "> 
      <%= image_tag @user.profileImage ,:class => "z-depth-2"%>
    </div>
		<div class="rate ">
			<a class="like-btn add-btn btn-floating btn-large waves-effect waves-light blue darken-1 z-depth-2"><i class="material-icons">thumb_up</i></a>
		</div>
		<div class="add ">
			<a class="add-btn btn-floating btn-large waves-effect waves-light red z-depth-2"><i class="material-icons">add</i></a>
		</div>
	</div>
	<div class="details">
		<h3><%= @user.name %></h3>
	</div>
</main>

<footer class="page-footer blue z-depth-4">
          <div class="container">
            <div class="row">
              <div class="col l6 s12">
                <h5 class="white-text">CrowdCircle</h5>
                <p class="grey-text text-lighten-4">Prince & Kunal</p>
              </div>
              <div class="col l4 offset-l2 s12">
              </div>
            </div>
          </div>
          <div class="footer-copyright">
            <div class="container">
            &copy; 2017 Copyright
            <a class="grey-text text-lighten-4 right" href="#!">Terms</a>
            </div>
          </div>
</footer>
</div>
<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.1/js/materialize.min.js"></script>








